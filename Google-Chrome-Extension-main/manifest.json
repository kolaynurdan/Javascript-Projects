{
    "name": "Follower Assistant",
    "version":1.0,
    "manifest_version":3,
    "background": {
        "scripts": ["./background.js"]
    },
    "icons":{
        "128":"icons8-128x128.png"
    },
    "browser_action" : {
        "default_popup" : "popup.html",
        "default_icon": "icons8-19x19.png"
    },
    "permissions":[
        "tabs",
        "https://www.instagram.com/<user_name>"
    ]

}